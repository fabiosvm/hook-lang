
let arr = [5, 2, 7, 3, 0, 1, 4, 6];

let n = 8;
let has_changed;
do {
  has_changed = false;
  n--;
  for (let i = 0; i < n; i++) {
    if (arr[i] > arr[i + 1]) {
      let t = arr[i];
      arr[i] = arr[i + 1];
      arr[i + 1] = t;
      has_changed = true;
    }
  }
} while (has_changed);

echo arr;
