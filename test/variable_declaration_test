#!/bin/sh

. test/__test

title "Variable declaration"

script "const a = 1;
const [b, c] = [2, 3];
let d;
let e = 4;
const [f, g] = ['foo', 'bar'];
let [h] = ['baz'];
let [i, j] = ['qux', 'quux'];

const {k, l} = {k: 'foo', l: 'bar'};
let {m} = {m: 'baz'};
let {n, o, p} = {n: 'qux', p: 'quux'};

println(a);
println(b);
println(c);
println(d);
println(e);
println(f);
println(g);
println(h);
println(i);
println(j);
println(k);
println(l);
println(m);
println(n);
println(o);
println(p);"

expect_success "1
2
3
null
4
foo
bar
baz
qux
quux
foo
bar
baz
qux
null
quux"

run
