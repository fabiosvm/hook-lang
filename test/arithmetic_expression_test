#!/bin/sh

. test/_test

title "Arithmetic expression"

script "println(123 + 1.23);
println(123 - 1.23);
println(123 * 1.23);
println(123 / 1.23);
println(123 % 12.3);
println(-123);
println(-1.23);
println(--123);
println(123 + 1.23 * 4);
println((123 + 1.23) * 4);"

expect_success "124.23
121.77
151.29
100
12.3
-123
-1.23
123
127.92
496.92"

run
